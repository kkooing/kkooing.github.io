  <span class="nt">&lt;thead&gt;</span>
    <span class="nt">&lt;tr&gt;</span>
      <span class="nt">&lt;th&gt;</span>h1<span class="nt">&lt;/th&gt;</span>
      <span class="nt">&lt;th&gt;</span>h2<span class="nt">&lt;/th&gt;</span>
